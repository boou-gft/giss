<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:element name="solicitarReprocessamentoPorMunicipioRequest">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="codigoEstado" type="xsd:int" default="0" />
        <xsd:element name="competenciaInicial" type="xsd:int" default="0" />
        <xsd:element name="competenciaFinal" type="xsd:int" default="0" />
        <xsd:element name="usuario" type="xsd:string" />
        <xsd:element name="criterioSelecao" type="xsd:string" />
        <xsd:element name="municipios" minOccurs="720" maxOccurs="720">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="codigoMunicipio" type="xsd:long" default="0" />
              <xsd:element name="codigoGrupo" type="xsd:int" default="0" />
              <xsd:element name="codigoSubgrupo" type="xsd:int" default="0" />
              <xsd:element name="percAliquotaApuracaoEvtal" type="xsd:decimal" default="0" />
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
        <xsd:element name="tipoUsuario" type="xsd:string" />
        <xsd:element name="usuarioCanal" type="xsd:string" />
        <xsd:element name="codigoCanal" type="xsd:int" default="0" />
        <xsd:element name="terminal" type="xsd:string" />
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>


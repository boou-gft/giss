<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:element name="listarPedidoBalancetesResponse">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="codMensagem" type="xsd:string" />
        <xsd:element name="mensagem" type="xsd:string" />
        <xsd:element name="qtdeRegistros" type="xsd:int" default="0" />
        <xsd:element name="ocorrencias" minOccurs="0" maxOccurs="50">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:element name="codSolicitacaoBalancete" type="xsd:long" default="0" />
              <xsd:element name="codTipoBalancete" type="xsd:string" />
              <xsd:element name="dtInicioPesquisa" type="xsd:int" default="0" />
              <xsd:element name="dtFimPesquisa" type="xsd:int" default="0" />
              <xsd:element name="descUf" type="xsd:string" />
              <xsd:element name="descEmpresa" type="xsd:string" />
              <xsd:element name="descMunicipio" type="xsd:string" />
              <xsd:element name="codSituacaoPedido" type="xsd:string" />
              <xsd:element name="descSituacaoPedido" type="xsd:string" />
              <xsd:element name="hrInclusaoPedido" type="xsd:string" />
              <xsd:element name="codDependencia" type="xsd:int" />
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
</xsd:schema>


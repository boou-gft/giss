        <xsd:element name="movtoReceitaApuracaoTotalSinal" type="xsd:string" />
        <xsd:element name="movtoReceitaApuracaoTotalValor" type="xsd:double" default="0" />
        <xsd:element name="movtoReceitaTributavelTotalSinal" type="xsd:string" />
        <xsd:element name="movtoReceitaTributavelTotalValor" type="xsd:double" default="0" />
        <xsd:element name="tributoApurcaoTotalSinal" type="xsd:string" />
        <xsd:element name="tributoApurcaoTotalValor" type="xsd:double" default="0" />

              <xsd:element name="movtoReceitaApuracaoValor" type="xsd:double" default="0" />
              <xsd:element name="movtoReceitaTributavelSinal" type="xsd:string" />
              <xsd:element name="movtoReceitaTributavelValor" type="xsd:double" default="0" />
              <xsd:element name="percAliquotaPadrao" type="xsd:double" default="0" />
              <xsd:element name="tributoApurcaoSinal" type="xsd:string" />
              <xsd:element name="tributoApurcaoValor" type="xsd:double" default="0" />
